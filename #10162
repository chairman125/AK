#include <stdio.h>

int main(void) {
    int A=300,B=60,C=10;
    int T=0;
    scanf("%d",&T);
    if(T%C > 0) {
        printf("-1");
        return 0;
    }
    else {
        printf("%d ",T/A);
        printf("%d ",(T%A)/B);
        printf("%d",(T%B)/C);
    }
    return 0;
}

// Greedy, bronze 3
